@model Music.Views.Shared.SongList

<div class="songsList">
    <h3 class="mb-4">@Model.NameList</h3>
    <div class="track-list">
        @foreach (var song in Model.Songs)
        {
            <div class="track-item d-flex" data-src="@song.UrlSong">
                <div class="track-number">•</div>
                <div class="track-info">
                    <h6 class="track-info-title mb-0">@song.Name</h6>
                </div>
            </div>
        }
    </div>
</div>

